package main

import (
	"fmt"
	"log"
	"strconv"

	"github.com/benburkert/pubsub"
)

func main() {
	ps, err := pubsub.New[string](16, 4)
	if err != nil {
		log.Fatal(err)
	}

	subs := []string{"A", "B", "C", "D"}
	for i := range subs {
		id := subs[i]
		fn := func(v string) {
			fmt.Printf("%s got %s\n", id, v)
		}
		ps.SubFunc(fn)
	}

	for i := 0; i <= 25; i++ {
		ps.Pub(strconv.Itoa(i))
	}
	ps.Close()
}
